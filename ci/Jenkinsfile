pipeline {
    agent {
        docker {
            image 'maven:3.6.2-jdk-11-openj9'
            args '-v maven-repo:/root/.m2'
        }
    }

    stages {
        stage('maven'){
            environment {
                STUDENTDATA_SECRET = credentials('STUDENTDATA_SECRET')
            }
            steps {
                sh 'mvn -B -DskipTests clean package'
            }
        }
    }
}